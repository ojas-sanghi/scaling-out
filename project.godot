CityInfo="*res://src/singletons/CityInfo.cs"
CombatInfo="*res://src/singletons/CombatInfo.cs"
DinoInfo="*res://src/singletons/DinoInfo.cs"
LaneInfo="*res://src/singletons/LaneInfo.cs"
PlayerStats="*res://src/singletons/PlayerStats.cs"

[debug]

gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep"

