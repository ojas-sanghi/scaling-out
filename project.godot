config_version=4

[application]

config/name="Scaling Out"
config/description="Project for DevFest 2020 Game Jam.

Go on undercover missions, upgrade your dinos, and destroy some cities!"
run/main_scene="res://src/GUI/screens/TitleScreen.tscn"
config/icon="res://assets/dinos/mega_dino/mega_dino.png"
config/windows_native_icon="res://assets/dinos/mega_dino.ico"

[audio]

default_bus_layout="res://src/default_bus_layout.tres"

[autoload]

Events="*res://src/singletons/Events.cs"
SceneChanger="*res://src/singletons/SceneChanger.tscn"
ShopInfo="*res://src/singletons/ShopInfo.cs"
